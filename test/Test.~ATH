//!/usr/bin/env ~ATH
/*
 Commented
 code
 test.
*/
print("Welcome to ~ATH.\n");
input TEST "Input name: "; // Raw string input.
print("Thank you, ~s for inputting.\n", TEST); // Raw string output.

PROCREATE CTR 5;
REPLICATE CT2 CTR;
BIFURCATE CTR[CT3, CT2];
AGGREGATE [CTR, CT3]CT2;
REPLICATE CTR NULL;
~ATH(CTR){
	print("Print ~d times.\n", 1);
	CTR.DIE();
	print("This should not print.");
} EXECUTE(NULL);
print("I wish you a peaceful death, ~s.\n", TEST);
THIS.DIE();
