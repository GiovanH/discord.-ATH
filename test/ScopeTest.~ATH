PROCREATE B 5;

FABRICATE FOO() {
	REPLICATE A (B + 5);
	DIVULGATE A;
}

FABRICATE BAR() {
	PROCREATE B 2;
	DIVULGATE EXECUTE(FOO);
}

PROCREATE LOOP 1;
~ATH(LOOP) {
	print ("Foo: ~d\n", EXECUTE(FOO)); //10
	print ("Bar: ~d\n", EXECUTE(BAR)); //7
	LOOP.DIE();
} EXECUTE(NULL);
THIS.DIE();
