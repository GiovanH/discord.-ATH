PROCREATE B 5;

FABRICATE FOO(NULL) {
	REPLICATE A B;
	A += 5;
	DIVULGATE A;
}

FABRICATE BAR(NULL) {
	PROCREATE B 2;
	EXECUTE(FOO);
}

PROCREATE LOOP 1;
~ATH(LOOP) {
	PROCREATE ANS EXECUTE(FOO, NULL);
	print ("Foo: ~d", ANS);
	PROCREATE ANS EXECUTE(BAR, NULL);
	print ("Bar: ~d\n", ANS);
	LOOP.DIE();
}
THIS.DIE();