PROCREATE B 5;

FABRICATE FOO() {
	REPLICATE A (B + 5);
	DIVULGATE A;
}

FABRICATE BAR() {
	PROCREATE B 2;
	REPLICATE A EXECUTE(FOO);
	DIVULGATE A;
}

PROCREATE LOOP 1;
~ATH(LOOP) {
	REPLICATE ANS EXECUTE(FOO);
	print ("Foo: ~d\n", ANS); //10
	REPLICATE ANS EXECUTE(BAR);
	print ("Bar: ~d\n", ANS); //7
	LOOP.DIE();
}
THIS.DIE();