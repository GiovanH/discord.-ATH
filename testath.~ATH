//!/usr/bin/env ~ATH
/*
 Commented
 code
 test.
*/
print("Welcome to ~ATH.");
input TEST "Input name: "; // Raw string input.
print("Thank you, ~s for inputting.", TEST); // Raw string output.

PROCREATE CTR 5;
REPLICATE CT2 CTR;
BIFURCATE CTR[CT3, CT2];
AGGREGATE [CTR, CT3]CT2;
REPLICATE CTR NULL;
~ATH(CTR){
	CTR.DIE();
	// print("This should not print.");
}
print("I wish you a peaceful death, ~s.", TEST);
THIS.DIE();
